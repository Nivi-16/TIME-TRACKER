{
  "manifest_version": 3,
  "name": "Productivity Time Tracker",
  "version": "2.0",
  "description": "Tracks website usage and analyzes productivity.",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
